<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.JSCorp.wp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.JSCorp.wp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.JSCorp.wp.DynamicBracketActivity"
            android:label="@string/title_activity_dynamic_bracket" >
        </activity>
        <activity
            android:name="com.JSCorp.wp.StaticBracketActivity"
            android:label="@string/title_activity_static_bracket" >
        </activity>
    </application>

</manifest>
